          }
        ]
      },
      "event": {
        "type": "tagging",
        "params": {
          "msgCode": "rbacProjectUpdated"
        }
      }
    },
    {
      "name": "rbacProjectUpdated",
      "conditions": {
        "any": [
          {
            "fact": "rbacProjectDeleted",
            "operator": "exist",
