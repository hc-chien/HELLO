{
  "rules": [
    {
      "name": "assetCreated",
      "conditions": {
        "all": [
          {
            "fact": "assetCreated",
            "operator": "exist",
            "value": true
          },
          {
            "fact": "level",
            "operator": "equal",
            "value": "info"
          }
        ]
      },
      "event": {
        "type": "tagging",
        "params": {
          "msgCode": "assetCreated"
        }
      }
    },
